{
  "name": "tg18pc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "dev": "vue-cli-service build --mode dev",
    "stage": "vue-cli-service build --mode stage",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "dll": "rimraf dll && webpack -p --progress --config webpack.dll.config.js",
    "test:unit": "vue-cli-service test:unit"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "root": true,
    "rules": {
      "no-tabs": "off",
      "max-len": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "no-trailing-spaces": 0,
      "no-multi-assign": 0,
      "import/no-extraneous-dependencies": 0,
      "no-useless-escape": 0,
      "no-underscore-dangle": 0,
      "prefer-destructuring": 0,
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "no-shadow": 0,
      "no-bitwise": 0,
      "camelcase": 0,
      "no-extra-parens": 0
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "aframe": "^0.8.2",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "bootstrap": "^4.1.1",
    "clipboard": "^2.0.1",
    "element-ui": "^2.4.6",
    "fingerprintjs2": "^1.8.0",
    "jwt-decode": "^2.2.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "object-hash": "^1.3.0",
    "socket.io-client": "^2.2.0",
    "swiper": "^4.3.5",
    "vue": "^2.5.17",
    "vue-analytics": "^5.14.0",
    "vue-awesome-swiper": "^3.1.3",
    "vue-axios": "^2.1.1",
    "vue-drag-verify": "^1.0.6",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-airbnb": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "add-asset-html-webpack-plugin": "^3.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-lodash": "^3.3.2",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.4",
    "generate-asset-webpack-plugin": "^0.3.0",
    "happypack": "^5.0.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "os": "^0.1.1",
    "pug": "^2.0.3",
    "pug-filters": "^3.1.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.0.1",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-cli": "^3.1.2",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
