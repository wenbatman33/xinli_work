{
  "name": "event18",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production",
    "stage": "vue-cli-service build --mode stage",
    "dev": "vue-cli-service build --mode dev",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "babel-plugin-lodash": "^3.3.2",
    "bootstrap": "^4.1.1",
    "clipboard": "^2.0.1",
    "compression-webpack-plugin": "^1.1.11",
    "element-ui": "^2.4.6",
    "fingerprintjs2": "^1.8.0",
    "firebase": "^5.7.2",
    "fs": "^0.0.1-security",
    "generate-asset-webpack-plugin": "^0.3.0",
    "glob": "^7.1.3",
    "jquery": "^3.3.1",
    "json-loader": "^0.5.7",
    "jwt-decode": "^2.2.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-sass": "^4.9.0",
    "npm": "^5.10.0",
    "object-hash": "^1.3.0",
    "popper.js": "^1.14.3",
    "pug": "^2.0.3",
    "pug-filters": "^3.1.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "reset-css": "^3.0.0",
    "sass-loader": "^7.0.1",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.21.0",
    "swiper": "^4.3.5",
    "vue": "^2.5.16",
    "vue-analytics": "^5.14.0",
    "vue-awesome-swiper": "^3.1.3",
    "vue-axios": "^2.1.1",
    "vue-i18n": "^8.0.0",
    "vue-qrcode-reader": "^1.3.3",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vuefire": "^1.4.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-airbnb": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.4",
    "glob": "^7.1.2",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "sass-loader": "^7.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-tabs": "off",
      "max-len": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "no-trailing-spaces": 0,
      "import/no-extraneous-dependencies": 0,
      "import/first": 0,
      "no-useless-escape": 0,
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "no-shadow": 0,
      "camelcase": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
